<h1>Bottomless ToDo List</h1>
<%= link_to "New List", new_list_path %> 
<% @lists.each do |list| %> 
<h3><%= link_to list.task, list %></h3>
<h4>Complete?<%= list.complete %></h4>
<div><h4>Due on<%=list.due_date%></h4></div>
<%= link_to "Edit List", edit_list_path(list)%>
<%= link_to 'destroy list', List, method: :delete, data: { confirm: "Are you sure your really done with the task, and not sweeping it under the rug?"} %>
<%end%>